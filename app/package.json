    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "newsapi": "^2.4.1",
    "node-cron": "^4.2.1",
    "openai": "^6.15.0",
    "redis": "^5.10.0",
